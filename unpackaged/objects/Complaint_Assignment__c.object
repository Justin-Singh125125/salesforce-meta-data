    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Closure_Info_Only_Edited_If_Closed</fullName>
        <active>true</active>
        <description>Prevent closure information from being changed when not closed.</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),TEXT(Status__c)  &lt;&gt; &apos;Closed&apos;,OR( 
ISCHANGED(Closure_Reason__c ), 
ISCHANGED(Closure_Violations_Found__c  ) ,
ISCHANGED(Closure_External_Partner__c ), 
ISCHANGED(Closure_Comments__c )))</errorConditionFormula>
        <errorMessage>You cannot change closure information if the status is not closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closure_Must_Have_Reason</fullName>
        <active>true</active>
        <description>Must Have Closure Reason When Closing A Complaint Assignment</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),ISPICKVAL(Status__c , &quot;Closed&quot;) &amp;&amp; ISPICKVAL(Closure_Reason__c, &quot;&quot;))</errorConditionFormula>
        <errorMessage>You must provide a closure reason when closing a complaint assignment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inv_By_BDO_Must_Answer_Violations</fullName>
        <active>true</active>
        <description>If Complaint Assignment Closure Reason is &quot;Investigated by BDO&quot;, Must Provide an answer to Violations Found Question</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),ISPICKVAL( Closure_Reason__c , &quot;Investigated By BDO&quot;),OR( ISPICKVAL( Closure_Violations_Found__c , &quot;&quot;), ISBLANK( Closure_Comments__c )))</errorConditionFormula>
        <errorMessage>You must select whether or not there were violations or provide closure comments if you select &quot;Investigated By BDO&quot; as a Closure Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inv_By_Ext_Agy_Ext_Part</fullName>
        <active>true</active>
        <description>If the Closure Reason is Investigated By External Agency You Must Provide an External Partner.</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),ISPICKVAL( Closure_Reason__c , &quot;Investigated By External Agency&quot;) &amp;&amp; ISBLANK( Closure_External_Partner__c ))</errorConditionFormula>
        <errorMessage>You must select the External Partner if you select &quot;Investigated By External Agency&quot; as a Closure Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inv_By_Ext_Agy_Vio_Found</fullName>
        <active>true</active>
        <description>If the Closure Reason is &quot;Investigated By External Agency&quot;, you must provide whether or not violations were found.</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),ISPICKVAL( Closure_Reason__c , &quot;Investigated By External Agency&quot;) &amp;&amp; ISPICKVAL( Closure_Violations_Found__c , &apos;&apos; ))</errorConditionFormula>
        <errorMessage>You must select whether or not violations were found if you select &quot;Investigated By External Agency&quot; as a Closure Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Must_Provide_Comments</fullName>
        <active>true</active>
        <description>If the Closure Reason is Other, Must Provide Closure Comments</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),ISPICKVAL( Closure_Reason__c , &quot;Other&quot;) &amp;&amp; ISBLANK( Closure_Comments__c ))</errorConditionFormula>
        <errorMessage>You must provide Closure Comments if you select &quot;Other&quot; as a Closure Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Certain_Fields_From_Being_Change</fullName>
        <active>true</active>
        <description>The Complaint, Type, and Group fields should not be changed.</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),NOT(ISNEW()),OR(ISCHANGED( Complaint__c ),ISCHANGED( Type__c ),ISCHANGED( Group__c )))</errorConditionFormula>
        <errorMessage>The Complaint Number, Assignment Type, and Complaint Assignment Group fields cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proper_Status_Flow_Closed_To_New</fullName>
        <active>true</active>
        <description>Cannot go back to New from Closed</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),PRIORVALUE(Status__c ) = &apos;Closed&apos;,Text(Status__c) = &apos;New&apos;)</errorConditionFormula>
        <errorMessage>You cannot move the status backwards from Closed to New.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proper_Status_Flow_Closed_To_Referred</fullName>
        <active>true</active>
        <description>Cannot go back to Referred from Closed</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),PRIORVALUE(Status__c ) = &apos;Closed&apos;,Text(Status__c) = &apos;Referred&apos;)</errorConditionFormula>
        <errorMessage>You cannot move the status backwards from Closed to Referred.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proper_Status_Flow_Closed_To_Under_Revie</fullName>
        <active>true</active>
        <description>Cannot go back to Under Review From Closed</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),PRIORVALUE(Status__c ) = &apos;Closed&apos;,Text(Status__c) = &apos;Under Review&apos;)</errorConditionFormula>
        <errorMessage>You cannot move the status backwards from Closed to Under Review.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proper_Status_Flow_Referred_To_New</fullName>
        <active>true</active>
        <description>Cannot go back to New from Referred</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),PRIORVALUE(Status__c ) = &apos;Referred&apos;,Text(Status__c) = &apos;New&apos;)</errorConditionFormula>
        <errorMessage>You cannot move the status backwards from Referred to New.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proper_Status_Flow_Referred_To_Under_Rev</fullName>
        <active>true</active>
        <description>Cannot go back to Under Review From Referred</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),PRIORVALUE(Status__c ) = &apos;Referred&apos;,Text(Status__c) = &apos;Under Review&apos;)</errorConditionFormula>
        <errorMessage>You cannot move the status backwards from Referred to Under Review.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proper_Status_Flow_Under_Review_To_New</fullName>
        <active>true</active>
        <description>Cannot go back to New from Under Review</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),PRIORVALUE(Status__c ) = &apos;Under Review&apos;,Text(Status__c) = &apos;New&apos;)</errorConditionFormula>
        <errorMessage>You cannot move the status backwards from Under Review to New.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transferred_Must_Select_Account</fullName>
        <active>true</active>
        <description>If the Closure Reason is Transferred To External Agency You Must Provide External Partner</description>
        <errorConditionFormula>AND(NOT($Setup.Disable_All_Validation_Rules__c.Value__c),ISPICKVAL( Closure_Reason__c , &quot;Transferred to External Agency&quot;) &amp;&amp; ISBLANK( Closure_External_Partner__c ))</errorConditionFormula>
        <errorMessage>You must the External Partner if you select &quot;Transferred to External Agency&quot; as a Closure Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_and_Group_Must_Match</fullName>
        <active>true</active>
        <description>The type of the group must match the type of the assignment.</description>
        <errorConditionFormula>Group_Type__c  &lt;&gt;  Text(Type__c)</errorConditionFormula>
        <errorMessage>The type of the group must match the type of the assignment.</errorMessage>
    </validationRules>
