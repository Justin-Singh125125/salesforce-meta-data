        <errorConditionFormula>OR( ISBLANK(TEXT(BDO__c)  ) , ISBLANK(TEXT(BDO_Suborganization__c)) )</errorConditionFormula>
        <errorMessage>You must select a BDO and a Division, Office, or Program.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BDO_User_Agency</fullName>
        <active>false</active>
        <description>If User is Agency, Assigned BDO must be Agency</description>
        <errorConditionFormula>AND( text($User.BDO__c) = &apos;Agency&apos;,text(BDO__c) = &apos;Agency&apos;)</errorConditionFormula>
        <errorDisplayField>BDO__c</errorDisplayField>
        <errorMessage>If User is Agency, Assigned BDO must be Agency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BDO_User_Agency2</fullName>
        <active>true</active>
        <errorConditionFormula>AND( text($User.BDO__c) &lt;&gt; &apos;Agency&apos;,TEXT(BDO__c)=&apos;Agency&apos;,NOT( text( BDO_Suborganization__c ) = &apos;Executive Office&apos;  || text(BDO_Suborganization__c) = &apos;Fiscal and Admin Programs&apos;))</errorConditionFormula>
        <errorMessage>BDO users can only assign Agency tasks to the Executive Office or Fiscal and Admin Services</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Date_Cannot_Be_In_The_Future</fullName>
        <active>true</active>
        <errorConditionFormula>Completed_Date__c  &gt; TODAY()</errorConditionFormula>
        <errorMessage>Completed date cannot be a future date.  It can be today&apos;s date or in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Date_Is_Required_If_Completed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Completed_Date__c),TEXT( Status__c )=&apos;Completed&apos;, NOT(ISCHANGED(Status__c)))</errorConditionFormula>
        <errorMessage>Completed date is required if task is completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Date_Must_Be_Empty_If_Not_Comp</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Completed_Date__c)),TEXT( Status__c )&lt;&gt;&apos;Completed&apos;,TEXT( Status__c )&lt;&gt;&apos;Withdrawn&apos;)</errorConditionFormula>
        <errorMessage>Completed Date can only be entered if Status is Completed or Withdrawn</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Concurrent</fullName>
        <active>true</active>
        <description>If Con, can&apos;t have Pred</description>
        <errorConditionFormula>ISPICKVAL( Task_Type__c , &apos;Concurrent&apos;)  &amp;&amp; Not(IsBlank( Predecessor__c ))</errorConditionFormula>
        <errorMessage>You cannot have a Predecessor when Task Type = Concurrent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Due_Date__c )</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>Due Date is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Needed_For_Back_to_Assigned</fullName>
        <active>true</active>
        <description>Reason needed for back to Requester</description>
        <errorConditionFormula>AND(TEXT(Status__c)=&apos;Back to Assigned&apos;,ISBLANK(Reason__c))</errorConditionFormula>
        <errorMessage>Reason is needed when sending Back to Assigned</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Needed_For_Back_to_Requester</fullName>
        <active>true</active>
        <description>Reason needed for back to Requester</description>
        <errorConditionFormula>AND(TEXT(Status__c)=&apos;Back to Requestor&apos;,ISBLANK(Reason__c))</errorConditionFormula>
        <errorMessage>Reason is needed when sending Back to Requester</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sequential</fullName>
        <active>true</active>
        <description>Must have a Pres if Seq</description>
        <errorConditionFormula>ISPICKVAL(Task_Type__c, &apos;Sequential&apos;)  &amp;&amp; IsBlank( Predecessor__c )</errorConditionFormula>
        <errorMessage>If the Task Type = Sequential, please pick a Predecessor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sequential_cannot_be_delegated</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Task_Type__c, &apos;Sequential&apos;) &amp;&amp; 
ISPICKVAL( Predecessor__r.Status__c , &apos;Pending&apos;) &amp;&amp; 
ISPICKVAL( Status__c , &apos;Delegated&apos;)</errorConditionFormula>
        <errorMessage>Sequential tasks cannot be delegated until the Predecessor is completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Date_Cannot_Be_In_The_Future</fullName>
        <active>true</active>
        <errorConditionFormula>Status_Date__c  &gt; TODAY()</errorConditionFormula>
        <errorMessage>Status date cannot be a future date.  It can be today&apos;s date or in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubTask_BDO_Related_Task</fullName>
        <active>true</active>
        <description>SubTask must be the same as the Related Task</description>
        <errorConditionFormula>text(BDO__c)  &lt;&gt;  text(Related_To__r.BDO__c) &amp;&amp;  Not(IsBlank(Related_To__c))</errorConditionFormula>
        <errorDisplayField>BDO__c</errorDisplayField>
        <errorMessage>BDO must be the same as the Related Task BDO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_BDO_User</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;System Admin&quot;,text($User.BDO__c) &lt;&gt; &apos;Agency&apos;, text( BDO__c ) &lt;&gt; text($User.BDO__c),text( BDO__c ) &lt;&gt; &apos;Agency&apos; )</errorConditionFormula>
        <errorDisplayField>BDO__c</errorDisplayField>
        <errorMessage>Please choose your BDO or Agency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Due_Date</fullName>
        <active>true</active>
        <description>Task due date can be no later than Assignment due date.</description>
        <errorConditionFormula>if(ISBLANK( Related_To__c ), Due_Date__c  &gt; CAMS_Assignment__r.Due_Date__c, Due_Date__c  &gt;  Related_To__r.Due_Date__c)</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>CAUTION : Task Due Date cannot be later than Assignment Due Date or the Related Task Due Date</errorMessage>
    </validationRules>
